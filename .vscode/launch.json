  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "启动ssr服务端",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}\\ssr", // 可执行程序的启动路径
      "program": "${workspaceFolder}\\ssr\\webpack\\launch.server.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "express-day03",
      "cwd": "${workspaceFolder}\\express\\day03", // 可执行程序的启动路径
      "program": "${workspaceFolder}\\express\\day03\\index.js",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "启动程序",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}\\uploadFile", // 可执行程序的启动路径
      "program": "${workspaceFolder}\\uploadFile\\server.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "mini-pack启动程序",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}\\mini-webpack\\mini-pack", // 可执行程序的启动路径
      "program": "${workspaceFolder}\\mini-webpack\\mini-pack\\pack.js"
    }
  ]
}