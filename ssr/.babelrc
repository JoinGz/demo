{
  "env": {
    "node":{
      "presets": [
        "@babel/preset-react"
      ]
    },
    "web": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "chrome": "70"
            },
            "useBuiltIns": "usage",
            "corejs": 3
          }
        ],
        "@babel/preset-react"
      ],
      "plugins": [
        [
          "@babel/plugin-transform-runtime",
          {
            "corejs": 3
          }
        ]
      ]
    }
  }
}